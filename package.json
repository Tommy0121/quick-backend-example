{
  "name": "web-server",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "egg-scripts start --title=egg-server-web-server  --workers=1",
    "stop": "egg-scripts stop --title=egg-server-web-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "docker-up": "docker-compose up -d",
    "docker-down": "docker-compose down",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "eslint . --ext .ts",
    "clean": "ets clean"
  },
  "dependencies": {
    "axios": "^0.21.4",
    "egg": "^2.6.1",
    "egg-cors": "^2.2.3",
    "egg-scripts": "^2.6.0",
    "form-data": "^4.0.0",
    "sharp": "^0.29.1",
    "stream-wormhole": "^1.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/sharp": "^0.29.2",
    "@types/supertest": "^2.0.0",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.16.0",
    "eslint": "^6.7.2",
    "eslint-config-egg": "^8.0.0",
    "tslib": "^1.9.0",
    "typescript": "^3.0.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "eslintIgnore": [
    "coverage"
  ],
